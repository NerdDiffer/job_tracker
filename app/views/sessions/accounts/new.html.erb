<div class='sessions'>
  <%= form_for(:session, url: login_path) do |f| %>
    <div class="field">
      <%= f.label :email %><br>
      <%= f.email_field :email %>
    </div>
    <div class="field">
      <%= f.label :password %><br>
      <%= f.password_field :password %>
    </div>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
    <div class="center_contents">
      <button type="submit" class="btn btn-default">Log in</button>
    </div>
  <% end %>
</div>
